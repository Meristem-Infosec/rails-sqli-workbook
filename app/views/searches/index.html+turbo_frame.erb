<%= turbo_frame_tag "query_builder" do %>
  <div>
    <% if @results %>
      <h2>Results</h1>
      <%= @result.inspect %>

    <% elsif @previous_params %>
      <h2>Params were:</h2>
      <%= @previous_params.inspect %>

    <% elsif @error %>
      <h2>Error Message:</h2>
      <%= @error.inspect %>

    <% end %>
  </div>
<% end %>